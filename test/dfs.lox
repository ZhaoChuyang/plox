class Node {
    init() {
        this.left = nil;
        this.right = nil;
        this.val = nil;
    }

    init(val) {
        this.left = nil;
        this.right = nil;
        this.val = val;
    }
}

var root = Node(0);
var n1 = Node(1);
var n2 = Node(2);
var n3 = Node(3);
var n4 = Node(4);
var n5 = Node(5);
var n6 = Node(6);

root.left = n1;
root.right = n2;
n1.left = n3;
n1.right = n4;
n3.left = n5;
n3.right = n6;

fun dfs(node) {
    if(node == nil) return;
    print node.val;
    dfs(node.left);
    dfs(node.right);
}

dfs(root);
